# MgII
mpirun -np 8 outflow_doppler_mgii 1.0
mv spec.dat Output/spec_MgII_b1.0.dat
mpirun -np 8 outflow_doppler_mgii 3.0 
mv spec.dat Output/spec_MgII_b3.0.dat
mpirun -np 8 outflow_doppler_mgii 10.0 
mv spec.dat Output/spec_MgII_b10.0.dat
mpirun -np 8 outflow_doppler_mgii 30.0
mv spec.dat Output/spec_MgII_b30.0.dat
mpirun -np 8 outflow_doppler_mgii 100.0
mv spec.dat Output/spec_MgII_b100.0.dat
